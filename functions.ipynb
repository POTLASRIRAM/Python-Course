    "## Positional and keyword arguments\n",
    "def args(*args,**kwargs):\n",
    "    '''This is an example for positional and keyword aruguments in functions'''\n",
    "    for i in args :\n",
    "        print(i)\n",
    "    for j,k in kwargs.items() :\n",
    "        \n",
    "        print(f\"{j} : {k}\")\n",
    "args(1,2,3,4,name = 'Sam',age = 32)"
